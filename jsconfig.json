{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src/**/*"
  ],
  "paths": {
    "midgard/*": ["src/midgard/*"],
    "environment": ["environment.js"]
  },
  "exclude": ["node_modules"],
}
